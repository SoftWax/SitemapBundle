<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="SoftWax\Bundle\SitemapBundle\Controller\RobotsController" class="SoftWax\Bundle\SitemapBundle\Controller\RobotsController" public="false">
            <tag name="controller.service_arguments"/>
        </service>

        <service id="SoftWax\Bundle\SitemapBundle\Controller\SitemapController" class="SoftWax\Bundle\SitemapBundle\Controller\SitemapController" public="false">
            <tag name="controller.service_arguments"/>
        </service>

        <service id="SoftWax\Bundle\SitemapBundle\Controller\SitemapSectionController" class="SoftWax\Bundle\SitemapBundle\Controller\SitemapSectionController" public="false">
            <tag name="controller.service_arguments"/>
        </service>
    </services>
</container>